{
  "name": "kevinsimper.dk",
  "version": "0.0.1",
  "description": "My personal blog",
  "main": "app.js",
  "scripts": {
    "watch-server": "webpack -w --progress --config webpack.server.config.js",
    "watch-client": "webpack-dev-server -w --progress --config webpack.client.config.js",
    "watch": "npm run watch-client & npm run watch-server",
    "compile-server": "NODE_ENV=production webpack --progress --config webpack.server.config.js",
    "compile-client": "NODE_ENV=production webpack --progress --config webpack.client.config.js",
    "build": "npm run compile-client && npm run compile-server",
    "post-install": "npm run build",
    "start": "DEBUG=express:* node dist/server.js",
    "start-watch": "nodemon --exec npm start -w ./dist/",
    "deploy": "docker-compose build && docker tag -f kevinsimperdk_web registry.blackbeard.io/kevinsimper/blog && docker push registry.blackbeard.io/kevinsimper/blog"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@bitbucket.org/KevinSimper/kevinsimper.dk.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "compression": "^1.6.1",
    "express": "^4.13.4",
    "history": "^2.0.0",
    "material-ui": "^0.14.4",
    "moment": "^2.11.2",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-router": "^2.0.0",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.3.0",
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.5.2",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "css-loader": "^0.23.1",
    "ejs-loader": "^0.2.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "image-webpack-loader": "^1.6.3",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "json-loader": "^0.5.4",
    "markdown-loader": "^0.1.7",
    "node-sass": "^3.4.2",
    "sass-loader": "^3.1.2",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  }
}
