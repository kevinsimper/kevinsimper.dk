{
  "name": "kevinsimper.dk",
  "version": "0.0.1",
  "description": "My personal blog",
  "main": "server.js",
  "scripts": {
    "watch-server": "webpack -w --progress --config webpack.server.config.js",
    "watch": "npm run watch-server",
    "compile-server": "NODE_ENV=production webpack --bail --progress --config webpack.server.config.js",
    "compile-client": "NODE_ENV=production webpack --bail --progress --config webpack.client.config.js",
    "build": "npm run compile-client && npm run compile-server",
    "post-install": "npm run build",
    "start": "node server.js",
    "deploy": "docker tag -f kevinsimperdk_web kevinsimper/blog && docker push kevinsimper/blog",
    "fix-lint": "prettier --write --no-semi --single-quote \"**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:kevinsimper/kevinsimper.dk.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.16.1",
    "compression": "^1.6.2",
    "express": "^4.14.0",
    "history": "^4.2.0",
    "moment": "^2.15.1",
    "opbeat": "^4.13.0",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-router": "^4.1.1",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "chokidar": "^1.6.0",
    "css-loader": "^0.28.0",
    "express-reload": "^1.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.4",
    "ignore-file-loader": "^1.0.0",
    "image-webpack-loader": "^3.3.0",
    "json-loader": "^0.5.4",
    "markdown-loader": "^2.0.0",
    "postcss-color-function": "^3.0.0",
    "postcss-loader": "^1.3.3",
    "precss": "^1.4.0",
    "prettier": "1.7.4",
    "source-map-support": "^0.4.2",
    "style-loader": "^0.16.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2",
    "webpack-hot-middleware": "^2.12.2"
  }
}
