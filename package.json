{
  "name": "kevinsimper.dk",
  "version": "0.0.1",
  "description": "My personal blog",
  "main": "server.js",
  "scripts": {
    "watch-server": "webpack -w --progress --config webpack.server.config.js",
    "watch-client": "webpack-dev-server -w --progress --config webpack.client.config.js",
    "watch": "npm run watch-client & npm run watch-server",
    "compile-server": "NODE_ENV=production webpack --progress --config webpack.server.config.js",
    "compile-client": "NODE_ENV=production webpack --progress --config webpack.client.config.js",
    "build": "npm run compile-client && npm run compile-server",
    "post-install": "npm run build",
    "start": "DEBUG=express:* node server.js",
    "deploy": "docker tag -f kevinsimperdk_web kevinsimper/blog && docker push kevinsimper/blog"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:kevinsimper/kevinsimper.dk.git"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.13.1",
    "compression": "^1.6.2",
    "express": "^4.13.4",
    "history": "^3.0.0",
    "moment": "^2.13.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "opbeat": "^3.17.2",
    "react-router": "^2.4.1",
    "serve-favicon": "^2.3.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.4.0",
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.5.2",
    "babel-core": "^6.9.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "chokidar": "^1.6.0",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "image-webpack-loader": "^1.8.0",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "json-loader": "^0.5.4",
    "markdown-loader": "^0.1.7",
    "node-sass": "^3.7.0",
    "sass-loader": "^3.2.0",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
